{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"npm run dev:web\" \"npm run dev:worker\" \"npm run dev:client\"",
    "dev:web": "cross-env NODE_ENV=development PROCESS_TYPE=web tsx server/web.ts",
    "dev:worker": "cross-env NODE_ENV=development PROCESS_TYPE=worker tsx server/worker.ts",
    "dev:server": "cross-env NODE_ENV=development tsx server/index.ts",
    "dev:server:5001": "cross-env NODE_ENV=development PORT=5001 tsx server/index.ts",
    "dev:server:5002": "cross-env NODE_ENV=development PORT=5002 tsx server/index.ts",
    "dev:server:5003": "cross-env NODE_ENV=development PORT=5003 tsx server/index.ts",
    "dev:server:5004": "cross-env NODE_ENV=development PORT=5004 tsx server/index.ts",
    "dev:server:5005": "cross-env NODE_ENV=development PORT=5005 tsx server/index.ts",
    "dev:server:5006": "cross-env NODE_ENV=development PORT=5006 tsx server/index.ts",
    "dev:server:5007": "cross-env NODE_ENV=development PORT=5007 tsx server/index.ts",
    "dev:server:5008": "cross-env NODE_ENV=development PORT=5008 tsx server/index.ts",
    "dev:server:5009": "cross-env NODE_ENV=development PORT=5009 tsx server/index.ts",
    "dev:server:5010": "cross-env NODE_ENV=development PORT=5010 tsx server/index.ts",
    "dev:microservices": "node server/dev-microservices.js",
    "dev:client": "cd client && vite",
    "dev:client:3000": "cd client && cross-env VITE_PORT=3000 vite",
    "dev:client:3001": "cd client && cross-env VITE_PORT=3001 vite",
    "dev:client:3002": "cd client && cross-env VITE_PORT=3002 vite",
    "dev:client:3003": "cd client && cross-env VITE_PORT=3003 vite",
    "dev:client:3004": "cd client && cross-env VITE_PORT=3004 vite",
    "dev:client:3005": "cd client && cross-env VITE_PORT=3005 vite",
    "dev:client:3006": "cd client && cross-env VITE_PORT=3006 vite",
    "dev:client:3007": "cd client && cross-env VITE_PORT=3007 vite",
    "dev:client:3008": "cd client && cross-env VITE_PORT=3008 vite",
    "dev:client:3009": "cd client && cross-env VITE_PORT=3009 vite",
    "dev:client:3010": "cd client && cross-env VITE_PORT=3010 vite",
    "dev:5001-3000": "concurrently \"npm run dev:server:5001\" \"npm run dev:client:3000\"",
    "dev:5002-3001": "concurrently \"npm run dev:server:5002\" \"npm run dev:client:3001\"",
    "dev:5003-3002": "concurrently \"npm run dev:server:5003\" \"npm run dev:client:3002\"",
    "dev:5004-3003": "concurrently \"npm run dev:server:5004\" \"npm run dev:client:3003\"",
    "dev:5005-3004": "concurrently \"npm run dev:server:5005\" \"npm run dev:client:3004\"",
    "build": "npm run build:client && npm run build:web && npm run build:worker",
    "build:client": "cd client && cross-env ROLLUP_DISABLE_NATIVE=1 vite build",
    "build:web": "esbuild server/web.ts --platform=node --packages=external --bundle --format=esm --outdir=dist --outfile=dist/web.js --target=es2022 --tsconfig=tsconfig.json",
    "build:worker": "esbuild server/worker.ts --platform=node --packages=external --bundle --format=esm --outdir=dist --outfile=dist/worker.js --target=es2022 --tsconfig=tsconfig.json",
    "build:server": "esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist --target=es2022 --tsconfig=tsconfig.json",
    "start": "NODE_ENV=production node dist/index.js",
    "start:web": "NODE_ENV=production PROCESS_TYPE=web node dist/web.js",
    "start:worker": "NODE_ENV=production PROCESS_TYPE=worker node dist/worker.js",
    "check": "npm run type-check",
    "lint": "eslint server/src client/src --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint server/src client/src --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write .",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "verify": "npm run lint && npm run type-check && npm run build:client && npm run build:server && jest --coverage",
    "r2:sync": "node --loader tsx --no-warnings ./scripts/r2-sync.js",
    "imports:check": "node scripts/check-imports.cjs",
    "db:push": "drizzle-kit push",
    "db:setup": "cd server && npm run db:setup",
    "db:migrate": "cd server && npm run db:migrate",
    "db:seed": "cd server && npm run db:seed",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:setup": "./scripts/test-setup.sh",
    "test:voice": "./scripts/test-voice-agent.sh",
    "test:e2e": "cypress run || echo 'Cypress suite optional'",
    "test:performance": "artillery run tests/performance/api-load-test.yml",
    "demo:setup": "./scripts/demo-setup.sh",
    "demo:data": "node demo-data-creator.js",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:dev:logs": "docker-compose -f docker-compose.dev.yml logs -f",
    "docker:dev:down": "docker-compose -f docker-compose.dev.yml down",
    "docker:prod": "docker-compose up -d",
    "docker:reset": "docker-compose -f docker-compose.dev.yml down -v && docker-compose -f docker-compose.dev.yml up -d",
    "prepare": "husky install",
    "embed-worker": "node server/scripts/run-embed-worker.js",
    "embed-test": "tsx server/src/scripts/embed-test.ts"
  },
